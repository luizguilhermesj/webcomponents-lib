each inputs in locals.sources['forms-input']
    h2 #{inputs.title}
    each input in inputs.types
        div.form__group
            label(class="form__label" for=`input-${input.attr}`) #{input.attr}
            input(id=`input-${input.attr}`, name= `input-${input.attr}` type=input.attr class=`form__input`)
